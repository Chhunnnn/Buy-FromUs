<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chhun.buyfromus.CategoryList">

    <Button
        android:id="@+id/cat_list_btn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="153dp"
        android:layout_marginTop="46dp"
        android:text="Button1"
        android:onClick="goToCatList1"/>

    <Button
        android:id="@+id/cat_list_btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/cat_list_btn1"
        android:layout_below="@+id/cat_list_btn1"
        android:layout_marginTop="29dp"
        android:text="Button2" />

    <Button
        android:id="@+id/cat_list_btn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/cat_list_btn2"
        android:layout_below="@+id/cat_list_btn2"
        android:layout_marginTop="22dp"
        android:text="Button3" />

    <Button
        android:id="@+id/cat_list_btn4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/cat_list_btn3"
        android:layout_below="@+id/cat_list_btn3"
        android:layout_marginTop="24dp"
        android:text="Button4" />
</RelativeLayout>
